{
  "name": "julienfalque/symfony-service-replacer",
  "type": "library",
  "description": "Allows replacing services in a Symfony Dependency Injection Container at runtime",
  "license": "MIT",
  "require": {
    "php": "^7.3 || ^8.0",
    "laminas/laminas-code": "^3.3.1 || ^4.0",
    "ocramius/proxy-manager": "^2.2",
    "psr/container": "^1.0",
    "symfony/config": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/expression-language": "^5.0",
    "symfony/http-kernel": "^5.0"
  },
  "require-dev": {
    "composer/package-versions-deprecated": "^1.11",
    "phpunit/phpunit": "^9.5",
    "symfony/framework-bundle": "^5.0",
    "symfony/proxy-manager-bridge": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "JulienFalque\\SymfonyServiceReplacer\\": "src"
    },
    "exclude-from-classmap": [
      "/src/Test/",
      "/src/**Test.php"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
